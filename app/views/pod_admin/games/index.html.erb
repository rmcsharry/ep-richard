<h1>Games</h1>

<div class="row">
  <div class="col-md-5">
    <h4>Default set</h4>
    <div class="list-group">
    <%- @default_games.each_with_index do |game, index| %>
      <%= render 'shared/admin_game_item', object: game %>
    <%- end %>
    </div>
  </div>
</div>

<div class="row">
  <div class="col-md-5">
    <h4>Games released weekly</h4>
    <div class="list-group">
    <%- @weekly_games.each_with_index do |game, index| %>
      <%= render 'shared/admin_game_item', object: game %>
    <%- end %>
    </div>
  </div>
</div>
